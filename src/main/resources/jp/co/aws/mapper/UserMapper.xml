<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jp.co.aws.mapper.UserMapper">
    <insert id="insertUser">
        INSERT INTO
            user
            (Id,
            userName,
            password)
        VALUES(
            #{userId},
            #{userName},
            #{password}
        )
    </insert>
</mapper>